                 question number .1

#include <iostream>

using namespace std;
void prime(int n)
{
    int i,j;
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0){
            cout<<"not prime number";
            break;
        }

    }
    if(2*i-1==n)
        cout<<"prime number";

}
int main()
{
    int n;
    cout<<"enter a number";
    cin>>n;
    prime(n);
        return 0;
}

                                                 QUESTION NUMBER .2
                                                 
                                                 
  #include <iostream>

using namespace std;
void cheakmax(int n)
{
    int i,max=0,rem;
    while(n>0)
    {
        rem=n%10;
        if(rem>max)
            max=rem;
        n/=10;
    }
    cout<<"maximum number is="<<max;
}
int main()
{
    int n;
    cout<<"enter a number";
    cin>>n;
    cheakmax(n);
        return 0;
}

