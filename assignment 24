                 question number .1

#include <iostream>

using namespace std;
void prime(int n)
{
    int i,j;
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0){
            cout<<"not prime number";
            break;
        }

    }
    if(2*i-1==n)
        cout<<"prime number";

}
int main()
{
    int n;
    cout<<"enter a number";
    cin>>n;
    prime(n);
        return 0;
}

                                                 QUESTION NUMBER .2
                                                 
                                                 
  #include <iostream>

using namespace std;
void cheakmax(int n)
{
    int i,max=0,rem;
    while(n>0)
    {
        rem=n%10;
        if(rem>max)
            max=rem;
        n/=10;
    }
    cout<<"maximum number is="<<max;
}
int main()
{
    int n;
    cout<<"enter a number";
    cin>>n;
    cheakmax(n);
        return 0;
}

                                       QUESTION NUMBER 4
                                       
#include<iostream>
using namespace std;
int fact(int m);
int pascal(int x,int y)
{
    long f;
    f=fact(x)/(fact(y)*fact(x-y));
    return f;
}
int fact(int m)
{
    long fac=1;
    while(m>0){
        fac=fac*m;
        m--;
    }
        return fac;
    
}
int main()
{
    int n;
    cout<<"enter line where print pascal tringle";
    cin>>n;
    int i,j;
    for(i=0;i<=n;i++)
    {
        for(j=0;j<=i;j++)
        {
            cout<<pascal(i,j);
        }
        cout<<endl;
    }
}

                                QUESTION NUMBER  .5
                                
 #include<iostream>
#include<math.h>
using namespace std;
int cheak(int x)
{
    int m,c;
    m=sqrt(5*x*x-4);
    c=sqrt(5*x*x+4);
    if(m*m==(5*x*x-4)||c*c==(5*x*x+4))
    return 1;
    else
    return 0;
}
int main()
{
    int n;
    cout<<"enter line where print pascal tringle";
    cin>>n;
    cheak(n);
    if(cheak(n)==1)
    cout<<"fibbonocci series";
    else
    cout<<"not fibonocci series";
    return 0;
}



                                            QUESTION NUMBER .6
                                            
                                            
#include<iostream>
#include<math.h>
using namespace std;
void swap(int &x,int &y)
{
      int temp;
      temp=x;
      x=y;
      y=temp;
}
int main()
{
    int a,b;
    cout<<"enter a and b";
    cin>>a>>b;
    cout<<"before swaping";
    cout<<endl<<"a= "<<a<<"  "<<"b="<<b;
    swap(a,b);
    cout<<"a="<<a<<"  b="<<b;
    return 0;
}

                                                           QUESTION NUMBER . 7
                                                           
                                                           
#include<iostream>
#include<math.h>
using namespace std;
int sum(int x,int y,int z=0)
{
    int su;
   su=x+y+z;
   return su;
}
int main()
{
    int a,b;
    cout<<"enter a and b";
    cin>>a>>b;
    cout<<endl<<"sum="<<sum(a,b);
    cout<<endl<<"add three number";
    int c;
    cout<<"enter a number for c";
    cin>>c;
    cout<<endl<<"sum= "<<sum(a,b,c);
    return 0;
}


                                                  QUESYION NUMBER 8
                                                  
                                                  
#include <iostream>

using namespace std;
float area(int m)
{
    return 3.14*m*m;
}
int area(int a,int b)
{
    return a*b;
}
 area(float l,float h)
{
    return 1/2*(l*h);
}
int main()
{
    int r;
    cout<<"enter radius";
    cin>>r;
    cout<<"area of circle is="<<area(r);
    int a,b;
    cout<<endl<<"enter length and breadth";
    cin>>a>>b;
    cout<<endl<<"area of rectungle is="<<area(a,b);
    float h,l;
    cout<<endl<<"enter length and hight";
    cin>>l>>h;
    cout<<endl<<"area of tringle is="<<area(l,h);
    return 0;
}


                                                    QUESTION NUMBER 9


#include <iostream>

using namespace std;
int max(int x,int y)
{
    if(x>y)
        return x;
    else
        return y;
}
float max(float x,float y)
{
    if(x>y)
        return x;
    else
        return y;
}
int main()
{
    float a,b,n;
    int x,y,m;
    cout<<endl<<"enter a number";
    cin>>a>>b;
    x=a;
    y=b;
    if((x>1&&y>1)&&x!=y){
            m=max(x,y);
            if(m==x)
        cout<<endl<<"maximum number is="<<a;
        else
            cout<<endl<<"maximum number is="<<b;
    }
    else{
        n=max(a,b);
        cout<<endl<<"maximum number is"<<n;
    }
    return 0;
}


                                                                 QUESTION NUMBER 10
                                                                 
                                                                 
#include <iostream>

using namespace std;
int sum(int a,int b)
{
    return a+b;
}
float sum(float x,float y)
{
    return x+y;
}
int main()
{
    int a,b;
    cout<<"enter two number";
    cin>>a>>b;
    cout<<endl<<"sum="<<sum(a,b);
    float x,y;
    cout<<"enter two number";
    cin>>x>>y;
    cout<<endl<<"sum is="<<sum(x,y);
    return 0;
}
                                                                 
